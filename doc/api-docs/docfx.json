{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Cassandra/**.csproj",
            "src/Extensions/Cassandra.AppMetrics/**.csproj"
          ],
		  "src": "../../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
          "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
	  {
		"files": [ "index.md", "toc.yml" ]
	  }
    ],
    "dest": "api-docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "statictoc",
	  "datastax-template"
    ],
	"globalMetadata": {
      "_appTitle": "DataStax C# Driver for Apache Cassandra",
      "_enableSearch": true,
	  "_disableContribution": true,
	  "_appFooter": "Â© DataStax, All rights reserved."
    },
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}